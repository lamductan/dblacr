<?xml version="1.0" encoding="UTF-8"?>
<CredentialStructure xmlns="http://www.zurich.ibm.com/security/idemix/credentialStructure"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xsi:schemaLocation="http://www.zurich.ibm.com/security/idemix/credentialStructure ../xsd/CredentialStructure.xsd">

	<References>
		<IssuerPublicKey>http://www.zurich.ibm.com/~pbi/idmx_2-3/utopia/ipk.xml</IssuerPublicKey>
	</References>

	<Attributes>
		<Attribute issuanceMode="hidden" name="firstName" type="string" />
		<Attribute issuanceMode="hidden" name="lastName" type="string" />
		<Attribute issuanceMode="hidden" name="dateOfBirth" type="int" />
	</Attributes>

	<Features />

	<Implementation>
		<AttributeOrder>
			<Attribute name="firstName">1</Attribute>
			<Attribute name="lastName">2</Attribute>
			<Attribute name="dateOfBirth">3</Attribute>
		</AttributeOrder>
	</Implementation>

</CredentialStructure>